{
  "name": "lametric-rss-formatter",
  "version": "1.0.0",
  "description": "Reads RSS feed and formats it into LaMetric compatible JSON",
  "keywords": [
    "lametric",
    "rss"
  ],
  "author": "Rauli Laine <rauli.laine@iki.fi>",
  "license": "Unlicense",
  "repository": "https://github.com/RauliL/lametric-rss-formatter.git",
  "bugs": {
    "url": "https://github.com/RauliL/lametric-rss-formatter/issues"
  },
  "engines": {
    "node": ">=12"
  },
  "main": "./index.js",
  "main:src": "./index.ts",
  "types": "./index.d.ts",
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "clean": "rimraf './index.js'",
    "prebuild": "yarn run clean",
    "prepack": "yarn run build"
  },
  "dependencies": {
    "rss-parser": "^3.10.0"
  },
  "devDependencies": {
    "@types/node": "^14.14.22",
    "@types/xml2js": "^0.4.8",
    "rimraf": "^3.0.2",
    "typescript": "^4.1.3",
    "yarn": "^1.22.10"
  }
}
